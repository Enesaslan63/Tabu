{
  "expo": {
    "name": "TabuFun",
    "slug": "TabuFun",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "versionCode": 22,
      "icon": "./assets/icon.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.muhammedenesaslan.TabuFun"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font",
      ["expo-build-properties", {
        "android": {
          "usesCleartextTraffic": true,
          "allowBackup": false
        }
      }]
    ],
    "extra": {
      "eas": {
        "projectId": "44fcaa37-0977-4476-86d8-f45579c2177f"
      }
    }
  }
}
